{
  "name": "thereiwas-frontend",
  "private": true,
  "version": "2025.1.17",
  "type": "module",
  "scripts": {
    "start": "vite",
    "preview": "vite preview",
    "test": "exit 0",
    "build": "tsc && vite build",
    "check-format": "prettier --list-different \"{{public,src,scripts,types},.}/**/*.{js,jsx,ts,tsx,md,scss,css,json,htm,html}\" .prettierrc",
    "format": "prettier --write \"{{public,src,scripts,types},.}/**/*.{js,jsx,ts,tsx,md,scss,css,json,htm,html}\" .prettierrc",
    "lint": "eslint --format stylish src/**/*.{ts,tsx}",
    "lint:fix": "eslint --format stylish src/**/*.{ts,tsx} --fix"
  },
  "dependencies": {
    "jose": "^5.9.6",
    "@mui/base": "^5.0.0-beta.68",
    "@mui/icons-material": "^6.4.0",
    "@mui/lab": "^6.0.0-beta.23",
    "@mui/material": "^6.4.0",
    "@mui/styles": "^6.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.2",
    "@emotion/babel-plugin": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "browserslist-to-esbuild": "^2.1.1"
  },
  "devDependencies": {},
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jest-environment-jsdom",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}